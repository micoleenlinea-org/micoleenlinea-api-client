{
  "name": "@micoleenlinea-org/api-client",
  "version": "1.0.0",
  "description": "API client compartido para MiColeEnLinea (web y mobile)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    },
    "./mutations": {
      "types": "./dist/mutations/index.d.ts",
      "import": "./dist/mutations/index.mjs",
      "require": "./dist/mutations/index.js"
    },
    "./queries": {
      "types": "./dist/queries/index.d.ts",
      "import": "./dist/queries/index.mjs",
      "require": "./dist/queries/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.mjs",
      "require": "./dist/config/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=5.0.0",
    "axios": ">=1.6.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "@types/node": "^20.0.0",
    "axios": "^1.6.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/micoleenlinea-org/micoleenlinea-api-client.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "micoleenlinea",
    "api-client",
    "react-query"
  ],
  "author": "MiColeEnLinea Team",
  "license": "UNLICENSED"
}
